@model Booking

<h2>Delete Booking</h2>

<h3>Are you sure you want to delete this item?</h3>
<div>
    <h4>Booking</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Booking Id</dt>
        <dd class="col-sm-10">@Model.BookingId</dd>

        <dt class="col-sm-2">Description</dt>
        <dd class="col-sm-10">@Model.Description</dd>

        <dt class="col-sm-2">Date</dt>
        <dd class="col-sm-10">@Model.Date</dd>

        <dt class="col-sm-2">PatientId</dt>
        <dd class="col-sm-10">@Model.PatientId</dd>

        <dt class="col-sm-2">EmployeeId</dt>
        <dd class="col-sm-10">@Model.EmployeeId</dd>

    </dl>

    <form asp-action="DeleteConfirmed" asp-route-id="@Model.BookingId" method="post">
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="Table" class="btn btn-secondary">Cancel</a>
    </form>
</div>